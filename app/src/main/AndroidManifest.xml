<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.redsponge.mycoolapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".project.ProjectActivity"/>
        <activity android:name=".utils.SettingsActivity"/>
        <activity android:name=".project.NewProjectActivity"/>
        <activity android:name=".project.ProjectViewActivity"/>
        <activity android:name=".login.RegisterActivity"/>
    </application>

</manifest>